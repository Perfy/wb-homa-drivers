#!/bin/bash
. /etc/wb_env.sh

CONFFILE=/etc/wb-homa-gpio.conf

case "$WB_VERSION" in
    "32" )
        BOARD_CONF="/etc/wb-homa-gpio.conf.wbsh3"
    ;;

    "KMON1" )
        BOARD_CONF="/etc/wb-homa-gpio.conf.mka3"
    ;;

    * )
        BOARD_CONF="/etc/wb-homa-gpio.conf.default"
    ;;
esac


ln -sf $BOARD_CONF $CONFFILE


#DEBHELPER#

