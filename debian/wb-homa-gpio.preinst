#!/bin/bash
. /etc/wb_env.sh

CONFFILE=/etc/wb-homa-gpio.conf


case "$WB_VERSION" in
    "32" )
        BOARD_CONF="/etc/wb-homa-gpio.conf.wbsh3"
    ;;

    * )
        BOARD_CONF="/etc/wb-homa-gpio.conf.default"
    ;;
esac


# overwrite BOARD_CONF with CONF_FILE
if [ -e  $CONFFILE ] ; then
    TMP=`mktemp`
    cp $CONFFILE $TMP
    cp $TMP $BOARD_CONF
    rm $TMP
fi





